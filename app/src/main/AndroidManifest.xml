<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.paigesoftware.transitiontutorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TransitionTutorial">
        <activity android:name=".transitionexample.TransitionActivity"></activity>
        <activity
            android:name=".sharedelement.SharedElementActivitySecond"
            android:theme="@style/Theme.MaterialComponents.Light.DarkActionBar" />
        <activity
            android:name=".sharedelement.SharedElementActivity"
            android:theme="@style/Theme.MaterialComponents.Light.DarkActionBar"></activity>
        <activity android:name=".muletiplesceneinoneactivity.MultipleScenesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>